<%= simple_form_for(@week) do |f| %>
  <%= f.error_notification %>

    <%= f.input :number %>
    <%= f.input :title, :input_html => { :class => "span9"} %>
    <%= f.input :intro, :input_html => { :rows => 20, :class => "span9 tinymce"} %>
    <%= f.input :active %>
    <%= f.button :submit %>

<% end %>
