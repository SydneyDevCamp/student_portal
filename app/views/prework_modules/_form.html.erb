<%= simple_form_for(@prework_module) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :order %>
    <%= f.input :name %>
    <%= f.input :intro %>
    <%= f.input :resources %>
    <%= f.input :forum_link %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
