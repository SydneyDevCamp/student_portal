<h2><%= action_name.humanize %> Category</h2>
      <%= simple_form_for @category do |f| %>
      <%= f.input :title %>
      <%= f.input :position %>
      <%= f.input :state, :label => "Active" %>
      <%= f.submit "submit", :class => "btn" %> or <%= link_to "cancel", forums_path %>
    <% end %>